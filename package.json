{
  "name": "dropbox-for-s3",
  "version": "0.0.1",
  "private": true,
  "description": "File sharing for developers. Share files from your own S3. Free when self-hosted and $2/mo for a private plan.",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node server",
    "watch": "npm run watch-css & npm run watch-js & DEBUG=instant* nodemon server",
    "watch-css": "stylus css/main.styl -o public/ -w",
    "build-css": "stylus css/main.styl -o public/ -c",
    "build-js": "NODE_ENV=production webpack",
    "watch-js": "webpack --watch",
    "build": "npm run build-css && npm run build-js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/twobucks/bucketly.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/twobucks/bucketly/issues"
  },
  "homepage": "https://github.com/twobucks/bucketly#readme",
  "dependencies": {
    "aws-sdk": "^2.85.0",
    "bluebird": "^3.5.0",
    "body-parser": "^1.17.2",
    "compression": "^1.6.2",
    "ejs": "^2.5.6",
    "emoji-favicon": "^0.3.0",
    "express": "^4.15.3",
    "multiparty": "^4.1.3",
    "pg": "^7.0.2",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.4.2",
    "sequelize-cli": "^2.7.0",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-preset-env": "^1.6.0",
    "nodemon": "^1.11.0",
    "stylus": "^0.54.5",
    "uglifyjs-webpack-plugin": "^0.4.6",
    "webpack": "^2.6.1"
  }
}
